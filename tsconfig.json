{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "experimentalDecorators": true,
  "include": [
    "src/", "index", "index-server.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}